{
      "compilerOptions": {
            "outDir": "./dist",
            "sourceMap": true,
            "strict": true,
            "module": "commonjs",
            "target": "es6",
            "esModuleInterop": true,
            "skipLibCheck": true,
            "forceConsistentCasingInFileNames": true
      },
      "include": ["src/**/*", "src/__tests__"],
      "exclude": ["node_modules", "**/*.test.ts", "**/__tests__", "**/__mocks__"]
}
